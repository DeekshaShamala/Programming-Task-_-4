        "def split_and_join(line):\n",
        "    l1=line.split(\" \")\n",
        "    l2=(\"-\").join(l1)\n",
        "    return l2\n",
        "    # write your code here\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    line = input()\n",
