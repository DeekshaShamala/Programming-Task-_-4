      "source": [
        "def mutate_string(string, position, character):\n",
        "    ls=list(string)\n",
        "    \n",
        "    ls[position]=character\n",
        "    \n",
        "    ls=\"\".join(ls)\n",
        "    \n",
        "    return ls\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    s = input()\n",
        "    i, c = input().split()\n",
        "    s_new = mutate_string(s, int(i), c)\n",
        "    print(s_new)"
      ]
    }
  ]
