      "source": [
        "def swap_case(s):\n",
        "    s1=\"\"\n",
        "    for i in range(len(s)):\n",
        "        \n",
        "        if s[i].isupper():\n",
        "            \n",
        "            s1+=s[i].lower()\n",
        "            \n",
        "        else:\n",
        "            \n",
        "            s1+=s[i].upper()\n",
        "        \n",
        "    return s1"
      ]
    }
  ]
}